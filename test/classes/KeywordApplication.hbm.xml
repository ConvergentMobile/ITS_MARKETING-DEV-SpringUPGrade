<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class 
name="keyword.KeywordApplication" 
table="keyword_application">
	
<id name="kwApplId" column="id" type="java.lang.Long">
<generator class="native"/>
</id>

<property
name="email"
type="java.lang.String"
column="email"
/>

<property
name="mobilePhone"
type="java.lang.String"
column="mobile_phone"
/>

<property
name="shortcode"
type="java.lang.String"
column="shortcode"
/>

<property
name="keyword"
type="java.lang.String"
column="keyword"
/>

<property
name="categoryId"
type="java.lang.Integer"
column="category_id"
/>

<property
name="status"
type="java.lang.String"
column="status"
/>

<property
name="businessName"
type="java.lang.String"
column="business_name"
/>

<property
name="repName"
type="java.lang.String"
column="rep_name"
/>

<property
name="repEmail"
type="java.lang.String"
column="rep_email"
/>

<property
name="repMobilePhone"
type="java.lang.String"
column="rep_mobile_phone"
/>

<property
name="customerField1"
type="java.lang.String"
column="customer_field_1"
/>

<property
name="customerField2"
type="java.lang.String"
column="customer_field_2"
/>

<property
name="customerField3"
type="java.lang.String"
column="customer_field_3"
/>

<property
name="promoCode"
type="java.lang.String"
column="promo_code"
/>

<property
name="acceptTerms"
type="java.lang.Boolean"
column="accept_terms"
/>

<property
name="siteId"
type="java.lang.Integer"
column="site_id"
/>
</class>
</hibernate-mapping>