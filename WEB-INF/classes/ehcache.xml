<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
 
    <defaultCache
                maxElementsInMemory="5000"
                eternal="false"
                overflowToDisk="false"
                memoryStoreEvictionPolicy="LFU" />
                
	<cache name="offices"
	      maxBytesLocalHeap="5M"
	      eternal="true"
	      memoryStoreEvictionPolicy="LRU">
	      <pinning store="localMemory"/>
	 </cache>                
</ehcache>