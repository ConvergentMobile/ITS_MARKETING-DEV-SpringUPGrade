<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="data.ApprovedMessage" table="approved_messages">
 <cache usage="read-write"/>

<id name="messageId" column="id" type="java.lang.Integer">
<generator class="native"/>
</id>

<property
name="userId"
type="java.lang.Long"
column="user_id"
/>

<property
name="siteId"
type="java.lang.Integer"
column="site_id"
/>

<property
name="messageName"
type="java.lang.String"
column="message_name"
/>

<property
name="messageText"
type="java.lang.String"
column="message_text"
/>

<property
name="language"
type="java.lang.String"
column="language"
/>

<property
name="status"
type="java.lang.String"
column="status"
/>

<property
name="comments"
type="java.lang.String"
column="comments"
/>

<property
name="entityId"
type="java.lang.String"
column="entity_id"
/>

<property
name="lastUpdated"
type="java.util.Date"
column="last_updated"
/>

<property
name="officeId"
type="java.lang.String"
column="office_id"
/>

<property
name="created"
type="java.util.Date"
column="created"
/>

<property
name="updated"
type="java.util.Date"
column="updated"
/>

<property
name="location"
type="java.lang.String"
column="location"
/>

</class>
</hibernate-mapping>